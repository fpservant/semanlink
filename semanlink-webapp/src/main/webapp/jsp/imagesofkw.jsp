<!--imagesofkw.jsp--><%/** * Affichage de la liste des images d'un keyword. * Gère aussi le cas où on affiche tous les descendants */%><%@ page language="java" session="true" import="net.semanlink.semanlink.*,net.semanlink.servlet.*,java.util.*"%><%@ taglib uri="http://struts.apache.org/tags-bean" prefix="bean" %><%@ taglib uri="http://struts.apache.org/tags-html" prefix="html" %><%@ taglib uri="http://struts.apache.org/tags-logic" prefix="logic" %><%Jsp_Keyword jsp = (Jsp_Keyword) request.getAttribute("net.semanlink.servlet.jsp");SLKeyword kw = jsp.getSLKeyword();Bean_DocList docList = jsp.getDocList();List list;String linkToThisKwPage = jsp.getLinkToThis();%><% ///////////////////////////////////////////  THUMBNAILS %><%{ // BLOC	list = docList.getImages();	int n = list.size();	for (int i = 0; i < n; i++) {		SLDocument doc = (SLDocument) list.get(i);		HTML_Link link = HTML_Link.linkToDocument(doc);%>		<div class="thumbnail">		<html:link page="<%=link.getPage()%>"><img src="<%=doc.getURI()%>" width="100" /></html:link>		<%=link.getLabel()%>		</div>	<%	} // for} // BLOC%><div class="clearboth"></div><!--/imagesofkw.jsp-->