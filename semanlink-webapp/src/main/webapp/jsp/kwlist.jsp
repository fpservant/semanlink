<%@ page language="java" session="true" import="net.semanlink.servlet.*" %><%@ taglib uri="http://struts.apache.org/tags-bean" prefix="bean" %><%@ taglib uri="http://struts.apache.org/tags-html" prefix="html" %><%@ taglib uri="http://struts.apache.org/tags-logic" prefix="logic" %><%/** * Suppose défini un bean Bean_KwList contenant la liste à afficher dans l'attribut de request "net.semanlink.servlet.Bean_KwList". */ %><!--kwlist.jsp--><%Bean_KwList truc = (Bean_KwList) request.getAttribute("net.semanlink.servlet.Bean_KwList");String propUri = (String) request.getAttribute("net.semanlink.servlet.rdfa.property"); // 2013-08 RDFa (on peut y mettre par ex skos:narrower)String contextPath = request.getContextPath(); // /semanlink -> la suite ne marche pas si appelée via snip sur autre siteString snip = request.getParameter("snip");if (snip != null) {    contextPath = SLServlet.getServletUrl();    }if (truc != null) {        int n = truc.size();        if (n > 0) {            String ulClass = truc.getUlCssClass();            if (ulClass != null) {                ulClass= " class=\"" + ulClass + "\"";            } else {                ulClass = "";            }                        // bordel d'ie (6) de merde : ne supporte pas les before / after ds les styles            // 2013-03: fuck ie 6                       %><ul<%=ulClass%>><%            if (propUri == null) {                for (int i = 0; i < n; i++) {                	System.out.println("AAAAA " + response.encodeURL(truc.getHREF(contextPath, i)));                    %><li><a href="<%=response.encodeURL(truc.getHREF(contextPath, i))%>"><%=truc.getLabel(i)%></a></li><%                } // for            } else { // 2013-08                      // RDFa I don't like it: we lose the direct link to the html (cf null arg) // TODO                for (int i = 0; i < n; i++) {                  System.out.println("BBBBB " + response.encodeURL(truc.getHREF(contextPath, i)));                    %><li><a property="<%=propUri%>" href="<%=response.encodeURL(truc.getHREF(contextPath, i, null))%>" typeof="skos:Concept"><span property="skos:prefLabel"><%=truc.getLabel(i)%></span></a></li><%                } // for            }            %></ul><%                    } else {            %>&nbsp;<%        }// if (n > 0) or not} // if (truc != null)%><!--/kwlist.jsp-->