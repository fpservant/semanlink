<!--thesauri.jsp--><%@ page language="java" session="true" import="net.semanlink.semanlink.*,net.semanlink.servlet.*,net.semanlink.util.*, java.util.*"%><%@ taglib uri="http://struts.apache.org/tags-bean" prefix="bean" %><%@ taglib uri="http://struts.apache.org/tags-html" prefix="html" %><%@ taglib uri="http://struts.apache.org/tags-logic" prefix="logic" %><%SLModel model = SLServlet.getSLModel();%><ul>	<%	{		Vector v = model.getThesauri();		for (int i = 0; i < v.size(); i++) {			SLThesaurus th = (SLThesaurus) v.get(i);			HTML_Link link = HTML_Link.linkToThesaurus(th);			%><li><html:link page="<%=link.getPage()%>"><%=link.getLabel()%></html:link></li><%		}	}	%></ul><!--/thesauri.jsp-->