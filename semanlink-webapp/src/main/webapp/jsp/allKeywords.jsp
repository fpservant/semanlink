<!--allKeywords.jsp--><%/** * Suppose défini un bean Bean_KwList dans l'attribut de request "net.semanlink.servlet.Bean_KwList". * Attention, cet attribut est ici modifiŽ. */%><%@ page language="java" session="true" import="net.semanlink.servlet.*,net.semanlink.semanlink.*, java.util.*"%><%@ taglib uri="http://struts.apache.org/tags-bean" prefix="bean" %><%@ taglib uri="http://struts.apache.org/tags-html" prefix="html" %><%@ taglib uri="http://struts.apache.org/tags-logic" prefix="logic" %><%SLModel model = SLServlet.getSLModel();Bean_KwList truc = (Bean_KwList) request.getAttribute("net.semanlink.servlet.Bean_KwList");List kws = null;int n;if (false) {	kws = model.getKWsInRealSpaceArrayList();	n = kws.size();	truc.setList(kws);	%>	<div class="what">Keywords in real space - nb : <%=n%></div>	<div class="horizEnumeration" id="kwsinrealspace">	<jsp:include page="kwlist.jsp"/>	</div>			<%	kws = model.getKWsInConceptsSpaceArrayList();	n = kws.size();	truc.setList(kws);	%>	<div class="what">Keywords in concept space - nb : <%=n%></div>	<div class="horizEnumeration" id="kwsinconceptspace">	<jsp:include page="kwlist.jsp"/>	</div><%} // if false%><div class="what">Orphelins</div><%truc.setList(model.debugRealKWsNotInConceptSpace());%><div class="horizEnumeration"><jsp:include page="kwlist.jsp"/></div><!--/allKeywords.jsp-->