<?xml version='1.0' encoding='UTF-8'  ?><%@ page    contentType="text/xml;charset=UTF-8"     pageEncoding="UTF-8"	language="java"	session="true"	import="net.semanlink.servlet.*,net.semanlink.semanlink.*,net.semanlink.util.xml.*,java.util.*"%><%@ taglib uri="http://struts.apache.org/tags-bean" prefix="bean" %><%@ taglib uri="http://struts.apache.org/tags-html" prefix="html" %><%@ taglib uri="http://struts.apache.org/tags-logic" prefix="logic" %><%// SLKeyword kw = (SLKeyword) request.getAttribute("net.semanlink.servlet.SLKeyword");Jsp_RSS jsp = (Jsp_RSS) request.getAttribute("net.semanlink.servlet.jsp");// SLKeyword kw = (SLKeyword) jsp.getSLResource();// non, car link vers l'adresse type thesaurus// String link = kw.getURI();String link = jsp.getLink();List docs = jsp.getDocs();String s;%><rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns="http://purl.org/rss/1.0/" xmlns:dc="http://purl.org/dc/elements/1.1/">	<channel rdf:about="<%=jsp.getAbout()%>">		<title><%=XMLWriterUtil.xmlEscape(jsp.getTitle())%></title>		<link><%=link%></link>		<description><%=jsp.getDescription()%></description>		<items>			<rdf:Seq>			<%			for (int i = 0; i < docs.size(); i++) {				SLDocument doc = (SLDocument) docs.get(i);				%>				<rdf:li resource="<%=XMLWriterUtil.xmlEscape(doc.getURI())%>"/><%			}%>			</rdf:Seq>		</items>	</channel>	<%for (int i = 0; i < docs.size(); i++) {	SLDocument doc = (SLDocument) docs.get(i);	%>	<item rdf:about="<%=XMLWriterUtil.xmlEscape(doc.getURI())%>">		<title><%=XMLWriterUtil.xmlEscape(SLUtils.getLabel(doc))%></title>		<link><%=jsp.getLink(doc)%></link><%		 s = jsp.getComment(doc);		 if (s != null) { %>		<description><%=s%>		</description><%		}%>		<dc:date><%=jsp.getDate(doc)%></dc:date>	</item><%} // for docs%></rdf:RDF>