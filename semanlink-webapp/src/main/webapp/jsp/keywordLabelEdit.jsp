<!--keywordLabelEdit.jsp--><%/** * Edit the labels of a keyword //  2021-01 */%><%@ page language="java" session="true" import="net.semanlink.semanlink.*,net.semanlink.servlet.*,java.util.*"%><%@ taglib uri="http://struts.apache.org/tags-bean" prefix="bean" %><%@ taglib uri="http://struts.apache.org/tags-html" prefix="html" %><%@ taglib uri="http://struts.apache.org/tags-logic" prefix="logic" %><%
Jsp_Keyword jsp = (Jsp_Keyword) request.getAttribute("net.semanlink.servlet.jsp");SLKeyword kw = (SLKeyword) jsp.getSLResource();LabelLN lln = kw.getLabelLN();String lab = lln.getLabel();String lang = lln.getLang();request.setAttribute("net.semanlink.servlet.jsp.lang", lang);%><div class="graybox">	<div class="what"><%=jsp.i18l("tag.label")%></div>	<p></p>	<p><b>Preferred Label:</b></p>	<html:form action="setkwlabel">		<html:text property="kwlabel" value="<%=lab%>" size="60"/>		<html:hidden property="kwuri" value="<%=kw.getURI()%>" />		<jsp:include page="/jsp/langSelect.jsp" flush="true" />		<html:submit property="okBtn"><%=jsp.i18l("properties.setValue")%></html:submit>	</html:form>    <p><b>Alternate Labels:</b></p>    <%    List<LabelLN> alts = kw.getAltLabels();    for (LabelLN alt : alts) {    	lab = alt.getLabel();    	lang = alt.getLang();    	request.setAttribute("net.semanlink.servlet.jsp.lang", lang);    	    	%>    	<html:form action="changetriple" method="POST">	        <html:hidden property="cur_s" value="<%=kw.getURI()%>" />	        <html:hidden property="cur_p" value="<%=SLVocab.ALT_LABEL_PROPERTY%>" />	        <html:hidden property="cur_o" value="<%=lab%>" />	        <html:hidden property="cur_lang" value="<%=lang%>" />	                    <html:hidden property="s" value="<%=kw.getURI()%>" />            <html:hidden property="p" value="<%=SLVocab.ALT_LABEL_PROPERTY%>" />            <html:text property="o" value="<%=lab%>" size="60"/>            <jsp:include page="/jsp/langSelect.jsp" flush="true" />		    <html:hidden property="docorkw" value="kw" />            <html:hidden property="otype" value="lit" />		    <html:hidden property="redirect_uri" value="<%=kw.getURI()%>" />		    <html:submit property="<%=Action_SetOrAddProperty.SET%>"><%=jsp.i18l("properties.setValue")%></html:submit>	    </html:form>    	    	<%    }    %>    <html:form action="changetriple" method="POST">	    <html:hidden property="s" value="<%=kw.getURI()%>" />	    <html:hidden property="p" value="<%=SLVocab.ALT_LABEL_PROPERTY%>" />	    <html:text property="o" value="" size="60"/>	    <%request.setAttribute("net.semanlink.servlet.jsp.lang", null);%>   	    <jsp:include page="/jsp/langSelect.jsp" flush="true" />	    <html:hidden property="docorkw" value="kw" />        <html:hidden property="otype" value="lit" />	    <html:hidden property="redirect_uri" value="<%=kw.getURI()%>" />	    <html:submit property="<%=Action_SetOrAddProperty.SET%>"><%=jsp.i18l("properties.addValue")%></html:submit>    </html:form>            </div> <!-- class="graybox" --><!--/keywordLabelEdit.jsp-->