icher un bandeau "New Tags" que si on s'appuie sur la sl:dateCreation              // (pas si dc:Date)              if (net.semanlink.semanlink.SLVocab.SL_CREATION_DATE_PROPERTY.equals(SLServlet.getDefaultDateProperty())) {                  %><div class="what"><%=jsp.aboutTagList()%></div><%              }              %>                            <div class="horizEnumeration" style="margin-top:4px">                 <%-- <div class="horizEnumerationTitle"><%=jsp.aboutTagList()%>:</div> --%>                 <jsp:include page="/jsp/kwlist.jsp" flush="true" />              </div><%            }         }%>    </div> <!--keyword-->    <div class= "xdocs"><div class="graybox">		<div class="what"><%=jsp.aboutDocList()%></div>		<% /////////////////////////////////////////// NEW DOCS %>		<jsp:include page="doclist.jsp"/>	</div></div><% } %><% /////////////////////////////////////////// %><!--/thisMonth.jsp-->